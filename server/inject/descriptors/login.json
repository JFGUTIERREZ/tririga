{
  "type": "object",
  "comment": "Login",
  "properties": {
    "loginframe": {
      "type": "object",
      "properties": {
        "username": {
          "type": "string",
          "control": "text",
          "selector": "#userNameField"
        }
      },
      "frameSelector": "iframe[role=\"presentation\"]"
    },
    "islogin": {
      "type": "identifier",
      "selector": "#userNameField",
      "control": "text"
    }
  },
  "connect": [
    {
      "match": [
        "islogin"
      ],
      "screens": [
        "login"
      ]
    }
  ],
  "selector": "div#loginLogo"
}
