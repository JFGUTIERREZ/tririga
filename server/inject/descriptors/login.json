{
  "type": "object",
  "comment": "Login",
  "properties": {
    "loginframe": {
      "type": "object",
      "properties": {
        "username": {
          "type": "string",
          "control": "text",
          "selector": "#userNameField"
        },
        "password": {
          "type": "string",
          "control": "text",
          "selector": "#passwordField"
        },
        "submit": {
          "type": "action",
          "control": "action.button",
          "selector": ".loginButton"
        },
        "errorMessage": {
          "type": "string",
          "selector": "#loginError",
          "control": "property.innerText"
        }
      },
      "frameSelector": "iframe[role=\"presentation\"]"
    }
  },
  "connect": [],
  "frameSelector": "[src=\"index.jsp\"]"
}
